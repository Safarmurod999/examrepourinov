{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off",
        "button-name": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-webkit-text-size-adjust"
        ]
      }
    ],
    "axe/forms": [
      "default",
      {
        "label": "off",
        "select-name": "off"
      }
    ],
    "button-type": "off",
    "axe/keyboard": [
      "default",
      {
        "nested-interactive": "off"
      }
    ]
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not ios_saf <= 14.8",
    "not samsung <= 18",
    "not samsung <= 18"
  ]
}